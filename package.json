{
	"name": "ram-fe",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/node": "^12.0.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.8",
		"history": "^5.0.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.0.0-beta.0",
		"react-scripts": "4.0.3",
		"typescript": "^4.1.2",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"start": "concurrently '$npm_execpath run watch:css' 'react-scripts start'",
		"build": "npm run compile:css && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"compile:css": "postcss src/tailwind.css -o src/index.css",
		"watch:css": "chokidar --initial './src/**/*.tsx' -c '$npm_execpath run compile:css'"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/jest": "^26.0.15",
		"autoprefixer": "^9",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^6.2.1",
		"postcss": "^8.3.6",
		"postcss-cli": "^8.3.1",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat"
	}
}